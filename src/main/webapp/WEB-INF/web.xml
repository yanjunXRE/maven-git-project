<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd" version="3.1">
  <display-name>devopsproject</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
    <welcome-file>index.htm</welcome-file>
    <welcome-file>index.jsp</welcome-file>
    <welcome-file>default.html</welcome-file>
    <welcome-file>default.htm</welcome-file>
    <welcome-file>default.jsp</welcome-file>
  </welcome-file-list>
  <servlet>
 <servlet-name>UserServlet</servlet-name>
 <servlet-class>UserServlet</servlet-class>
 </servlet>

  <servlet>
 <servlet-name>GuideServlet</servlet-name>
 <servlet-class>GuideServlet</servlet-class>
 </servlet>
   <servlet>
 <servlet-name>RestaurantV2Servlet</servlet-name>
 <servlet-class>RestaurantV2Servlet</servlet-class>
 </servlet>

 


 <servlet-mapping>
 <servlet-name>UserServlet</servlet-name>
<url-pattern>/UserServlet/dashboard</url-pattern>
<url-pattern>/UserServlet/login</url-pattern>
 <url-pattern>/UserServlet/edit</url-pattern>
 <url-pattern>/UserServlet/update</url-pattern>
 <url-pattern>/UserServlet/delete</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 <servlet-name>GuideServlet</servlet-name>
<url-pattern>/GuideServlet/dashboard</url-pattern>

 <url-pattern>/GuideServlet/edit</url-pattern>
 <url-pattern>/GuideServlet/update</url-pattern>
 <url-pattern>/GuideServlet/delete</url-pattern>
 </servlet-mapping>
 <servlet-mapping>
 <servlet-name>RestaurantV2Servlet</servlet-name>
<url-pattern>/RestaurantV2Servlet/dashboard</url-pattern>
 <url-pattern>/RestaurantV2Servlet/edit</url-pattern>
 <url-pattern>/RestaurantV2Servlet/update</url-pattern>
 <url-pattern>/RestaurantV2Servlet/delete</url-pattern>


 </servlet-mapping>
</web-app>